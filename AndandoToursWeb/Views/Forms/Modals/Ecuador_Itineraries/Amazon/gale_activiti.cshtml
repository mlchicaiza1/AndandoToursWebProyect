@{
    var contenido = ViewBag.GetContenidoVista;
    var imagenItem = ViewBag.GetContenidoMult;
    var titulo = new List<string>();
    Dictionary<string, int> uniqueStore = new Dictionary<string, int>();

    foreach (var item in ViewBag.GetContenidoVista)
    {
            titulo.Add(item.Titulo);
    }
}
<div class="modal fade" id="gale2-activity" style="overflow:hidden;padding-right:0" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">
    <div class="modal-dialog modal-full" style="width:100%;height:100%; margin:0" role="document">
        <div class="modal-content" style="width:100%; height:100%">
            <div class="modal-header" style="overflow-y:hidden;height:60px">
                <img src="~/images/logos/andando-tours-png-azul-horizontal.png" style="width:100px; height:50px; margin-top:-10px; margin-left:0px" />
                <button type="button" class="close-form pull-right" onclick="limp();" data-dismiss="modal" style="margin-top:-40px">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body tamModal style-scroll" style="overflow-x:hidden; overflow-y:scroll;font-family:Arial;text-align:left">
                <div class="container">
                    <h3 class="formato-titulo" style="color:#5299D0; text-align:center">
                        @titulo[27]
                    </h3>
                    <div>
                        @{int contparrafo = 0;
                            foreach (var item in ViewBag.GetContenidoVista)
                            {
                                if (item.Titulo == titulo[27])
                                {

                                    contparrafo++;
                                    if (contparrafo == 2)
                                    {
                                        <h4 class="textoinfo formato-titulo" style="color:#5299D0;padding:25px 0">@Html.Raw(item.Parrafo)</h4>
                                    }
                                }
                            }
                        }
                        <div id="grid-gale-social" style="margin-left:-12px;">
                            <div id="grid-prod-galery-ext">
                                <div id="caja-galeria-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[27].UrlImagen" alt="@imagenItem[27].NombreImagen" style="width:750px; height:380px; margin-right:0" />
                                </div>
                                <div id="caja-galeria2-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[28].UrlImagen" alt="@imagenItem[28].NombreImagen" style="height:380px;" />
                                </div>
                                <div id="caja-galeria3-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[30].UrlImagen" alt="@imagenItem[30].NombreImagen" style="margin-left:12px; height:380px;" />
                                </div>
                                <div id="caja-galeria4-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[29].UrlImagen" alt="@imagenItem[29].NombreImagen" style="width:750px; height:380px; margin-right:0" />
                                </div>
                            </div>
                        </div>
                        @* ============MOBIL======== *@
                        <div id="grid-gale-social-mob">
                            <div id="grid-prod-galery-ext">
                                <div id="caja-galeria-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[27].UrlImagen" alt="@imagenItem[27].NombreImagen" />
                                </div>
                                <div id="caja-galeria2-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[28].UrlImagen" alt="@imagenItem[28].NombreImagen" />
                                </div>
                                <div id="caja-galeria3-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[30].UrlImagen" alt="@imagenItem[30].NombreImagen" />
                                </div>
                                <div id="caja-galeria4-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[29].UrlImagen" alt="@imagenItem[29].NombreImagen" />
                                </div>
                            </div>
                        </div>
                        @{int contparrafo2 = 0;
                            foreach (var item in ViewBag.GetContenidoVista)
                            {
                                if (item.Titulo == titulo[27])
                                {

                                    contparrafo2++;
                                    if (contparrafo2 == 3)
                                    {
                                        <h4 class="textoinfo formato-titulo" style="color:#5299D0;padding:25px 0">@Html.Raw(item.Parrafo)</h4>
                                    }
                                }
                            }
                        }
                        <div class="row">
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[31].UrlImagen" alt="@imagenItem[31].NombreImagen" />
                            </div>
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[32].UrlImagen" alt="@imagenItem[32].NombreImagen" />
                            </div>
                        </div>
                        <div class="row" style="margin-top:17px">
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[33].UrlImagen" alt="@imagenItem[33].NombreImagen" />
                            </div>
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[34].UrlImagen" alt="@imagenItem[34].NombreImagen" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>