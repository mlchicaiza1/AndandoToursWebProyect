@{

    var contenido = ViewBag.GetContenidoVista;
    var imagenItem = ViewBag.GetContenidoMult;
    var titulo = new List<string>();
    Dictionary<string, int> uniqueStore = new Dictionary<string, int>();

    foreach (var item in ViewBag.GetContenidoVista)
    {
            titulo.Add(item.Titulo);
    }

}
<div class="modal fade" id="gale-exterior" style="overflow:hidden;padding-right:0" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">
    <div class="modal-dialog modal-full" style="width:100%;height:100%; margin:0" role="document">
        <div class="modal-content" style="width:100%; height:100%">
            <div class="modal-header" style="overflow-y:hidden;height:60px">
                <img class="lazyload" src="~/images/logos/andando-tours-png-azul-horizontal.png" style="width:100px; height:50px; margin-top:-10px; margin-left:0px" />
                <button type="button" class="close-form pull-right" onclick="limp();" data-dismiss="modal" style="margin-top:-40px">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body tamModal style-scroll" style="overflow-x:hidden; overflow-y:scroll;font-family:Arial;text-align:left">
                <div class="container">
                    <h3 style="color:#5299D0; text-align:center">
                        @titulo[25]
                    </h3>
                    <div>
                        @{var cont = 0;
                            foreach (var item in ViewBag.GetContenidoVista)
                            {
                                if (item.Titulo == titulo[25])
                                {
                                    <span class="textoinfo parrafo formato-parrafo">@Html.Raw(item.Parrafo)</span>

                                    cont++;
                                    if (cont == 1)
                                    { break; }
                                }
                            }
                        }
                        <table id="grid-gale-social" style="margin-left:-14px;margin-top:25px;margin-bottom:25px">
                            <tr>
                                <td style="border:none;padding-right:0">
                                    <img src="@imagenItem[17].UrlImagen" class="lazyload" alt="@imagenItem[17].NombreImagen" style=" height:380px; margin-left:2px" />
                                </td>
                                <td style="border:none;padding-right:0">
                                    <img src="@imagenItem[18].UrlImagen" class="lazyload" alt="@imagenItem[18].NombreImagen" style="width:628px; height:380px;" />
                                </td>
                            </tr>
                        </table>
                        @* ===============MOBILE=============== *@
                        <div id="grid-gale-social-mob">
                            <div class="container" style="width:auto; padding:0;margin-top:10px">
                                <div class="col-md-12" style="padding:0; margin-top: 10px;">
                                    <img src="@imagenItem[17].UrlImagen" alt="@imagenItem[17].NombreImagen" class="img-responsive lazyload" />
                                </div>
                                <div class="col-md-12" style="padding:0;margin-top: 10px;">
                                    <img src="@imagenItem[18].UrlImagen" alt="@imagenItem[18].NombreImagen" class="img-responsive lazyload" />
                                </div>
                            </div>
                        </div>
                        @{int contparrafo = 0;
                            foreach (var item in ViewBag.GetContenidoVista)
                            {
                                if (item.Titulo == titulo[25])
                                {

                                    contparrafo++;
                                    if (contparrafo == 2)
                                    {
                                        <span class="textoinfo parrafo formato-parrafo">@Html.Raw(item.Parrafo)</span>
                                    }
                                }
                            }
                        }
                        <div class="row" style="margin-top: 25px;margin-bottom:25px">
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[19].UrlImagen" alt="@imagenItem[19].NombreImagen" />
                            </div>
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[20].UrlImagen" alt="@imagenItem[20].NombreImagen" />
                            </div>
                        </div>
                        <div class="row" style="margin-top:17px;margin-bottom:25px">
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[21].UrlImagen" alt="@imagenItem[21].NombreImagen" />
                            </div>
                            <div class="col-xs-6">
                                <img class="img-responsive lazyload" src="@imagenItem[22].UrlImagen" alt="@imagenItem[22].NombreImagen" />
                            </div>
                        </div>
                        @{int contparrafo2 = 0;
                            foreach (var item in ViewBag.GetContenidoVista)
                            {
                                if (item.Titulo == titulo[25])
                                {

                                    contparrafo2++;
                                    if (contparrafo2 == 3)
                                    {
                                        <span class="textoinfo parrafo formato-parrafo">@Html.Raw(item.Parrafo)</span>
                                    }
                                }
                            }
                        }
                    <div id="grid-gale-social" style="margin-top: 25px;margin-bottom:25px;margin-left:-12px;">
                        <div id="grid-prod-galery-ext">
