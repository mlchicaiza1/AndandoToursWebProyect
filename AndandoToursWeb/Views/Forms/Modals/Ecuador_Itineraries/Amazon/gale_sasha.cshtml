                            <div id="caja-galeria3-ext">
                                <img class="img-responsive lazyload" src="@imagenItem[26].UrlImagen" alt="@imagenItem[26].NombreImagen" style="margin-left:12px; height:380px;" />
                            </div>
                            <div id="caja-galeria4-ext">
                                <img class="img-responsive lazyload" src="@imagenItem[25].UrlImagen" alt="@imagenItem[25].NombreImagen" style="width:750px; height:380px; margin-right:0" />
                            </div>
                        </div>
                    </div>
                        @* ============MOBIL======== *@
                        <div id="grid-gale-social-mob">
                            <div id="grid-prod-galery-ext">
                                <div id="caja-galeria-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[23].UrlImagen" alt="@imagenItem[23].NombreImagen" />
                                </div>
                                <div id="caja-galeria2-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[24].UrlImagen" alt="@imagenItem[24].NombreImagen" />
                                </div>
                                <div id="caja-galeria3-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[25].UrlImagen" alt="@imagenItem[25].NombreImagen" />
                                </div>
                                <div id="caja-galeria4-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[26].UrlImagen" alt="@imagenItem[26].NombreImagen" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>