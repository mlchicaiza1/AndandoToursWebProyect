@{

    var contenido = ViewBag.GetContenidoVista;
    var imagenItem = ViewBag.GetContenidoMult;
    var titulo = new List<string>();
    Dictionary<string, int> uniqueStore = new Dictionary<string, int>();

    foreach (var item in ViewBag.GetContenidoVista)
    {

        if (!uniqueStore.ContainsKey(item.Titulo))
        {
            uniqueStore.Add(item.Titulo, 0);
            titulo.Add(item.Titulo);
        }
    }

}
<div class="modal fade" id="gale2-social" style="overflow:hidden;padding-right:0" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">
    <div class="modal-dialog modal-full" style="width:100%;height:100%; margin:0" role="document">
        <div class="modal-content" style="width:100%; height:100%">
            <div class="modal-header" style="overflow-y:hidden;height:60px">
                <img src="~/images/logos/andando-tours-png-azul-horizontal.png" style="width:100px; height:50px; margin-top:-10px; margin-left:0px" />
                <button type="button" class="close-form pull-right" onclick="limp();" data-dismiss="modal" style="margin-top:-40px">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body tamModal style-scroll" style="overflow-x:hidden; overflow-y:scroll;font-family:Arial;text-align:left">
                <div class="container">
                    <h3 style="color:#5299D0; text-align:center">
                        @titulo[9]
                    </h3>
                    <div>
                        <section style="background-color:white">
                            @{var cont = 0;
                                foreach (var item in ViewBag.GetContenidoVista)
                                {
                                    if (item.Titulo == titulo[9])
                                    {
                                        <span class="textoinfo parrafo formato-parrafo">@Html.Raw(item.Parrafo)</span>

                                        cont++;
                                        if (cont == 1)
                                        { break; }
                                    }
                                }
                            }
                        </section>
                        <div id="grid-gale-social" style="margin-left:-12px;margin-top:20px;margin-bottom:20px">
                            <div id="grid-prod-galery-ext">
                                <div id="caja-galeria-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[7].UrlImagen" alt="@imagenItem[7].NombreImagen" style="width:750px; height:380px; margin-right:0" />
                                </div>
                                <div id="caja-galeria2-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[8].UrlImagen" alt="@imagenItem[8].NombreImagen" style="height:380px;" />
                                </div>
                                <div id="caja-galeria3-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[9].UrlImagen" alt="@imagenItem[9].NombreImagen" style="margin-left:12px; height:380px;" />
                                </div>
                                <div id="caja-galeria4-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[10].UrlImagen" alt="@imagenItem[10].NombreImagen" style="width:750px; height:380px; margin-right:0" />
                                </div>
                            </div>
                        </div>
                        @* ============MOBIL======== *@
                        <div id="grid-gale-social-mob">
                            <div id="grid-prod-galery-ext">
                                <div id="caja-galeria-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[7].UrlImagen" alt="@imagenItem[7].NombreImagen" />
                                </div>
                                <div id="caja-galeria2-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[8].UrlImagen" alt="@imagenItem[8].NombreImagen" />
                                </div>
                                <div id="caja-galeria3-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[9].UrlImagen" alt="@imagenItem[9].NombreImagen" />
                                </div>
                                <div id="caja-galeria4-ext">
                                    <img class="img-responsive lazyload" src="@imagenItem[10].UrlImagen" alt="@imagenItem[10].NombreImagen" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>