@{
    ViewData["Title"] = "availabilityModal";
    Layout = null;
}
<link rel="stylesheet" type="text/css" href="~/css/app.css" />
<link rel="stylesheet" type="text/css" href="~/css/styleAndando.css" />
<link rel="stylesheet" type="text/css" href="~/css/styleAndando03.css" />
<script src="~/lib/jquery/dist/jquery.js"></script>
<div class="modal fade" id="avaiModal" style="overflow:hidden;padding-right:0" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">
    <div class="modal-dialog modal-full" style="width:100%;height:100%; margin:0" role="document">
        <div class="modal-content" style="width:100%; height:100%">
            <div class="modal-header" style="height:60px">
                <img  src="~/images/logos/andando-tours-png-azul-horizontal.png" style="width:100px; height:50px; margin-top:-10px; margin-left:0px" />
                <button type="button" class="close-form pull-right" v-on:click="closeModalAvailability" data-dismiss="modal" style="margin-top:-40px" onclick="clean();">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="" style="height:100%;width:100%">
                <div class="modal-body modal-body-available font-modal" style="margin:0 0%;padding-top: 0;">
                    <div class="container " id="containerDispoBarco" style="padding-left:0">
                        <div class="col-md-4 col-lg-4 col-xs-12" id="modalImgDispo">
                            <div class="row parrafo">
                                <div class="col-xs-12 col-md-12 col-lg-12" >
                                    <h3 style="text-align:left" v-if="filDispoBarcos.length > 0">{{filDispoBarcos[0].barcoNombre}}</h3>
                                    <a class="align-content-center">
                                        <img style="margin: 0.5px;" v-if="filDispoBarcos.length > 0" :src="datoCardBarco[0].barcoImagen" />
                                    </a>
                                </div>
                                <div class="carouselText col-xs-12 col-md-12 col-lg-12">
                                    <div class="row f-15 text-justify" style="margin:0;" id="experience_text">
                                        @*<h3  style="margin-top: 8px; text-align:left; color:#398AC9" ><b>Sunday</b><br /> <span style="color:black"><b>WEST DAYS</b></span></h3>*@
                                        <div class="style-scroll style-scrollText" style="overflow-y:scroll">
                                            <p style=" text-align: left;margin-bottom:0; line-height:1.5; font-size:16px" v-if="filDispoBarcos.length > 0">
                                                {{datoCardBarco[0].barcoDescripcionEn}}
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div  id="blockSlide" class="col-md-4 col-lg-12 col-xs-12" style="display:none;margin-right:10px; margin-left:-10px">
                            <div class="row f-15 text-justify textoinfo parrafo" style="font-family:Arial; color: #656b6f !important; background:white;">
                                <div>
                                    <div id="myCarouselModalAv" class="carousel slide active" style="background-color:white; height:400px; " data-interval="10000000" aria-hidden="true" data-ride="carousel">
                                        <div class="carousel-inner" role="listbox">
                                            <div v-bind:class="index == 0 ? active : '',noActivo" v-for="(dispon,index) in filDispoBarcos" class="item imgen-carousel" style="height:50%;max-width:100%; background-image:url('../../wwwroot/images/Cruceros-Productos/Itinerarios/East/day-1-black-turtle-cove.jpg')">
                                                <div class="row parrafo">
                                                    <div class="col-md-6">
                                                        <div class="row f-15 text-justify" style="margin:0; padding: 2em 1em 0; " id="experience_text">
                                                            <h3><p style=" text-align:left; color:#398AC9"><b>{{dispon.itiCompTituloEn}}</b><br /> <span style="color:black"><b>{{dispon.itinNombre}} DAYS</b></span></p></h3>
                                                            <div class="style-scroll" style="overflow-y:scroll;height:200px">
                                                                <p style=" text-align: left;margin-bottom:0;  font-size:18px">
                                                                </p>
                                                                <p style=" text-align: left;margin-bottom:0; line-height:1.5em; font-size:18px">
                                                                    Private 
                                                                </p>
                                                            </div>
                                                            <br />
                                                            <table style="margin-left:-13px">
                                                                <tr>
                                                                    <td style="border:none;padding-right:0">
                                                                        <img src="~/images/icon/icon-walk.jpg" class="img-circle" style="background-color:rgba(0, 0, 0, 0.43);width:35px;height:35px" />
                                                                        <p style="font-size:14px;color:#656b6f">Walks & Hikes</p>
                                                                    </td>
                                                                    <td style="border:none; padding-top:-10px;padding-right:0 ">
                                                                        <img src="~/images/icon/icon-snork2.jpg" class="img-circle" style="background-color:rgba(0, 0, 0, 0.43); width:35px;height:35px" alt="Alternate Text" />
                                                                        <p style="font-size:14px; color:#656b6f">Snorkeling</p>
                                                                    </td>
                                                                    <td style="border:none;padding-right:0">
                                                                        <img src="~/images/icon/icon-snork.jpg" class="img-circle" style="background-color:rgba(0, 0, 0, 0.43); width:35px;height:35px" alt="Alternate Text" />
                                                                        <p style="font-size:14px; color:#656b6f">Dinghy Ride</p>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6" style="padding: 4em 1em;">
                                                        <a class="align-content-center" >
                                                            <img style="margin: 0.5px; height:368px" src="../../images/Cruceros-Productos/Itinerarios/East/day-2-maryanne-itinerary-east.jpg" />
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <a class="left carousel-control" style="background:none;left:-100px" href="#myCarouselModalAv" role="button" data-slide="prev">
                                            <span class="glyphicon glyphicon-chevron-left" style="color:#0077ba" aria-hidden="true"></span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="right carousel-control" style="background:none;right:-100px" href="#myCarouselModalAv" role="button" data-slide="next">
                                            <span class="glyphicon glyphicon-chevron-right" style="color:#0077ba" aria-hidden="true"></span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div>
                                </div>
                            </div><br/>
                            <div class="col-lg-12">
                                <a v-on:click="modalBlockDispo" style="color:#0077ba" href="#">Availability >>></a>
                            </div>
                        </div>
                        <div id="dispo-display" class="col-md-8 col-xs-12" style="margin-right:-10px">
                            <div class="col-md-12" style="padding-top:15px;padding-bottom:15px;width: 120%;">
                                <div class="col-xs-6 col-md-6" style="padding-left: 0">
                                    <p id="titulo_dispo">
                                        Available departures
                                    </p>
                                </div>
                                <div class="col-xs-6 col-md-6">
                                    <div class="dropdown" >
                                        <button class="btn btn-default dropdown-toggle" type="button" id="menu1" data-toggle="dropdown" style="font-family:Arial; font-size:16px"><p>Select your travel dates</p><i style="padding-left:10px" class="fas fa-angle-down"></i></button>
                                        <ul class="dropdown-menu" role="menu" aria-labelledby="menu1" style="width:100%; font-size:18px">
                                            <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Enero</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 style-scroll containerHeightDispo" style="overflow-y:scroll; padding:0; overflow-x:hidden;">
                                <div v-if="filDispoBarcos.length">
                                    <div class="container containerDispoItems" style="margin-top:15px;padding-left: 0px; border-bottom:groove;font-family:Arial; border-width:0.5px; border-bottom-color:rgba(0, 0, 0, 0.24)" v-for="dispon in filDispoBarcos">
                                        <div class="col-xs-6 col-md-4 col-lg-3" style="text-align:left; line-height:0.6">
                                            <p class="text-dispoFecha" style="color:black;font-size:16px">{{dispon.salFechaSalida | format_date}}</p>
                                            <p style="font-size:14px;">{{dispon.noches}} Night</p>
                                        </div>
                                        <div class="barcoNombre col-xs-6 col-md-4 col-lg-2" style="text-align:left; line-height:0.6">
                                            <p style="color:black;font-size:15px;line-height: 0.8;margin-bottom: 10px;">{{dispon.barcoNombre}}</p>
                                            <p style="font-size:14px; color:#398AC9;margin-bottom: 15px;line-height:1">{{dispon.itinNombre}}</p>
                                        </div>
                                        <div class="col-xs-6 col-md-4 col-lg-2" style="text-align:left; line-height:0.6" v-if="dispon.idBarco== 85 || dispon.idBarco==73 ">
                                            <p class="text-availability" style="color:black;font-size:16px">{{dispon.totalDisponibilidad}}</p>
                                            <p class="text-availability_1" style="font-size:14px;">Availability</p>
                                        </div>
                                        <div class="col-xs-6 col-md-4 col-lg-2" style="text-align:left; line-height:0.6" v-else>
                                            <p class="text-availability" style="color:black;font-size:16px">{{dispon.salDispoTotalOtrosBarcos}}</p>
                                            <p class="text-availability_1" style="font-size:14px;">Availability</p>
                                        </div>
                                        <div class="col-xs-6 col-md-4 col-lg-2" style="text-align:left; line-height:0.6">
                                            <p style="color:black;font-size:16px">$ {{dispon.cabinaPrecio1}}</p>
                                            <p style="font-size:14px">From</p>
                                        </div>
                                        <div class="bookingClass  col-xs-12 col-md-4 col-lg-3" style="text-align:left; border-right:none; line-height:0.6;padding-left:0">
                                            <p><a class="btn buttonRequest" :id="dispon.idSalida" v-on:click="getservicio" style="font-family:SegoeUI, Segoe UI; "><b>Request Booking</b></a><p>
                                            <p style="font-size:14px; color:#F29F67;margin-top:-6px">More Information</p>
                                            <div class="col-xs-6" style="padding-left: 0">
                                                <p><a :id="dispon.idSalida" class="btn buttonRequest" v-on:click="getservicio" style="font-family:SegoeUI, Segoe UI;font-size:13px;padding: 2px 5px; "><b>Request Booking</b></a></p>
                                            </div>
                                            <div class="col-xs-6" style="padding: 5px 0;">
                                                <p style="font-size:13px; color:#F29F67;margin-top: 4px;padding-left: 13px;">More Information</p>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-12" v-else>
                                    !NO EXISTE DISPONIBILIDAD!</>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @* ======================footer================================================================== *@
                <div class="modal-footer" id="modal_footer" >
                    <section style="background-color:#E5E5E5;padding:0 0">
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-6 col-md-6">
                                    <p class="footer-inf" style="text-align:left">
                                        Andando Tours is committed to ensuring your privacy. We dont sell the information provded. <br />
                                        <a style="color:#5299D0" href="#">Privacy Policy. </a>
                                        
                                    </p>
                                </div>
                                <div class="col-xs-6 col-md-6" >
                                    <p class="footerTerms" style="text-align:right">
                                        |&nbsp;&nbsp; TERMS & CONDITIONS &nbsp;&nbsp;|&nbsp;&nbsp;  PRIVACY POLICY  &nbsp;&nbsp;|
                                    </p>
                                    <p class="footerTermsMovile" style="text-align:right">
                                        PRIVACY POLICY <br />TERMS & CONDITIONS
                                    </p>
                                </div>
                            </div>
                        </div>
                    </section>
                        
                        <div class="container" style=" padding-top: 10px;">
                            <div class="row">
                                <div class="logos-wrapper">
                                    <div>
                                        <img src="~/images/logos/LATA-2012.png" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                    <div>
                                        <img src="~/images/logos/logo-ecuador.png" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                    <div>
                                        <img src="~/images/logos/Galapagos-Islands-Travel-Angermeyer-Cruises.png" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                    <div>
                                        <img src="~/images/logos/americas.png" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                    <div>
                                        <img src="~/images/logos/central-de-receptivos.png" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                    <div>
                                        <img src="~/images/logos/full_color_white_background_copy.jpg" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                    <div>
                                        <img src="~/images/logos/logo-qa.png" class="img-responsive " style="max-height:100px;" alt="LATA-2012" />
                                    </div>
                                </div>
                            </div>
                        </div>
                </div>
                
            </div>
        </div>
    </div>
</div>
<script>
    function ocultar() {
        document.getElementById('dispo_escritorio').style.display = 'none';
        document.getElementById('row_cabina').style.display = 'flex';
        document.getElementById('row_submit').style.display = 'flex';
    }
    function clean() {
        $(document.body).on('hidden.bs.modal', function () {
            $('.modal').removeData('bs.modal')
        });
    }
</script>




