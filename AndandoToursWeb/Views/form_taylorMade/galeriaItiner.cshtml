@{
    ViewData["Title"] = "galeriaItiner";
    Layout = null;
}
<link rel="stylesheet" href="~/css/owl.carousel.css" />
<link rel="stylesheet" href="~/css/owl.theme.default.css" />
<div class="modal fade" id="gale-ItinerModal" style="overflow-y:hidden; padding-right:0!important" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="width:100%; margin:6% 0">
            <div style="background-color:white;">
                <span class="btn icon-times-circle close-boton close2" data-dismiss="modal" style="top: 0;right: 1%;"></span><br /><br /><br />
                <div id="myCarousel1" class="carousel slide active" aria-hidden="true" data-ride="carousel" data-interval="10000">
                    <div class="carousel-inner" style="max-width:100%; " role="listbox">
                        <div class="item active imgen-carousel" style="max-width:100%; background-image:url('../../wwwroot/images/About/Infinity exterior.jpg')">
                            <img src="~/images/Cruceros-Productos/Itinerarios/East/day-1-black-turtle-cove.jpg" alt="sailing Galapagos" class="img-fluid" style="height:100%; width:100%;" />
                        </div>
                        <div class="item imgen-carousel" style=" max-width:100%; background-image:url(../../wwwroot/images/About/Family-cruise.jpg)">
                            <img src="~/images/Cruceros-Productos/Itinerarios/East/day-3-maryanne-itinerary-east.jpg" alt="sailing Galapagos" class="img-responsive" style="width:100%; height:100%;" />
                        </div>
                        <div class="item imgen-carousel" style="max-width:100%;background-image:url(../../wwwroot/images/About/galapagos-fund-wildaid-passion.jpg)">
                            <img src="~/images/Cruceros-Productos/Itinerarios/East/day-4-maryanne-itinerary-east.jpg" alt="sailing Galapagos" class="img-responsive" style="width:100%; height:100%;" />
                        </div>
                    </div>
                    <a class="left carousel-control" href="#myCarousel1" style="background:none;" role="button" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel1" style="background:none;" role="button" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/js/Forms.js"></script>
<script src="~/js/owl.carousel.js"></script>