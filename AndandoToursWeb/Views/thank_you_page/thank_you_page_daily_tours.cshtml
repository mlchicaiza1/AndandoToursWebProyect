@{
    var contenido = ViewBag.GetContenidoVista;
    var imagenItem = ViewBag.GetContenidoMult;
    var cardsEcuadorItin = ViewBag.GetCards;
    //var imagenItem = ViewBag.GetContenidoMult;
    var titulo = new List<string>();
    var tituloCard = new List<string>();
    Dictionary<string, int> uniqueStore = new Dictionary<string, int>();


    foreach (var item in ViewBag.GetContenidoVista)
    {

        if (!uniqueStore.ContainsKey(item.Titulo))
        {
            uniqueStore.Add(item.Titulo, 0);
            titulo.Add(item.Titulo);
        }
    }
    uniqueStore.Clear();
    foreach (var item in ViewBag.GetCards)
    {

        if (!uniqueStore.ContainsKey(item.Titulo))
        {
            uniqueStore.Add(item.Titulo, 0);
            tituloCard.Add(item.Titulo);
        }
    }

    //Quitar tags de los 3 Tours de cada Isla
    var textoCms = cardsEcuadorItin[9].Parrafo;
    string[] textCards = textoCms.Split('*');
    var cardsTours = new List<string>();
    var cardsTours_1 = new List<string>();
    var cardsTours_2 = new List<string>();
    var cardsTours_3 = new List<string>();
    var cardsTours_4 = new List<string>();
    foreach (var item in textCards)
    {

        cardsTours.Add(Regex.Replace(item, "<.*?>", String.Empty).Replace("\n", "").Replace("&nbsp;", "").Replace("&ndash;", ""));

    }
    textoCms = cardsEcuadorItin[10].Parrafo;
    textCards = textoCms.Split('*');
    foreach (var item in textCards)
    {

        cardsTours_1.Add(Regex.Replace(item, "<.*?>", String.Empty).Replace("\n", "").Replace("&nbsp;", "").Replace("&ndash;", ""));

    }
    textoCms = cardsEcuadorItin[11].Parrafo;
    textCards = textoCms.Split('*');
    foreach (var item in textCards)
    {

        cardsTours_2.Add(Regex.Replace(item, "<.*?>", String.Empty).Replace("\n", "").Replace("&nbsp;", "").Replace("&ndash;", ""));

    }
    textoCms = cardsEcuadorItin[12].Parrafo;
    textCards = textoCms.Split('*');
    foreach (var item in textCards)
    {

        cardsTours_3.Add(Regex.Replace(item, "<.*?>", String.Empty).Replace("\n", "").Replace("&nbsp;", "").Replace("&ndash;", ""));

    }
    textoCms = cardsEcuadorItin[13].Parrafo;
    textCards = textoCms.Split('*');
    foreach (var item in textCards)
    {

        cardsTours_4.Add(Regex.Replace(item, "<.*?>", String.Empty).Replace("\n", "").Replace("&nbsp;", "").Replace("&ndash;", ""));

    }
}
